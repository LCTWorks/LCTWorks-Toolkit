<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<_LCTWorksWinUILibDir>$(MSBuildThisFileDirectory)LCTWorks.WinUI\</_LCTWorksWinUILibDir>
	</PropertyGroup>
	<ItemGroup Condition="Exists('$(_LCTWorksWinUILibDir)')">
		<Content Include="$(_LCTWorksWinUILibDir)**"
				 Link="LCTWorks.WinUI\%(RecursiveDir)%(FileName)%(Extension)"
				 CopyToOutputDirectory="PreserveNewest" />
	</ItemGroup>
</Project>